<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="901.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Reports">
   <children>
      <TabPane layoutX="10.0" layoutY="10.0" prefHeight="353.0" prefWidth="878.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="appointTotalTab" text="Appointment Totals">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="365.0" prefWidth="778.0">
                     <children>
                        <TableView fx:id="appointTypeTable" layoutX="88.0" layoutY="46.0" prefHeight="230.0" prefWidth="291.0">
                          <columns>
                            <TableColumn fx:id="appointTotalType" prefWidth="159.7090301513672" text="Appointment Type" />
                            <TableColumn fx:id="appointTypeTotal" prefWidth="97.74554443359375" text="Total" />
                          </columns>
                        </TableView>
                        <TableView fx:id="appointMonthTable" layoutX="492.0" layoutY="46.0" prefHeight="230.0" prefWidth="291.0">
                           <columns>
                              <TableColumn fx:id="appointMonth" prefWidth="159.7090301513672" text="Appointment By Month" />
                              <TableColumn fx:id="appointMonthTotal" prefWidth="97.74554443359375" text="Total" />
                           </columns>
                        </TableView>
                     </children></AnchorPane>
            </content>
          </Tab>
          <Tab fx:id="contactScheduleTab" text="Contact Schedules">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="324.0" prefWidth="910.0">
                     <children>
                        <TableView fx:id="contactTable" layoutX="1.0" layoutY="45.0" prefHeight="279.0" prefWidth="879.0">
                          <columns>
                            <TableColumn fx:id="appointId" prefWidth="103.85455131530762" text="Appointment ID" />
                            <TableColumn fx:id="appointTitle" prefWidth="109.0909423828125" text="Title" />
                              <TableColumn fx:id="appointDescription" minWidth="0.0" prefWidth="109.96369934082031" text="Description" />
                              <TableColumn fx:id="appointContact" prefWidth="90.76361083984375" text="Contact" />
                              <TableColumn fx:id="appointType" prefWidth="73.30908203125" text="Type" />
                              <TableColumn fx:id="appointStart" prefWidth="108.21810913085938" text="Start Date/Time" />
                              <TableColumn fx:id="appointEnd" prefWidth="127.4180908203125" text="End Date/Time" />
                              <TableColumn fx:id="appointCustId" prefWidth="82.036376953125" text="Customer ID" />
                              <TableColumn fx:id="appointUserId" prefWidth="61.963623046875" text="User ID" />
                          </columns>
                        </TableView>
                        <ComboBox fx:id="contactCombo" layoutX="124.0" layoutY="8.0" onAction="#contactPopulate" prefWidth="150.0" />
                        <Label layoutX="14.0" layoutY="13.0" text="Select an Contact:" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
            <Tab fx:id="appointCountryTab" text="Customer Totals By Country">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="countryTable" layoutX="42.0" layoutY="36.0" prefHeight="250.0" prefWidth="372.0">
                          <columns>
                            <TableColumn fx:id="appointCountry" prefWidth="181.52727508544922" text="Country" />
                            <TableColumn fx:id="appointCountryTotal" prefWidth="169.30908203125" text="Customer Total" />
                          </columns>
                        </TableView>
                     </children></AnchorPane>
               </content>
            </Tab>
        </tabs>
      </TabPane>
      <AnchorPane layoutX="-1.0" layoutY="374.0" prefHeight="67.0" prefWidth="902.0">
         <children>
            <Button fx:id="backToMenu" layoutX="797.0" layoutY="1.0" mnemonicParsing="false" onAction="#backToMenu" text="Back to Menu" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
